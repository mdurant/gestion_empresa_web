	<queryString>
		<![CDATA[SELECT a.id_dservicios,
	a.id_eservicios,
	a.posicion,
	a.codigo,
	a.descripcion,
	a.cantidad,
	a.tipo_impuesto,
	a.valor_neto,
	a.valor_iva,
	a.valor_subtotal,
	b.numero,
	b.fecha_ingreso,
	b.neto,
	b.iva,
	b.total,
	c.nombre_proyecto,
	c.id_proyecto,
	b.usuario,
	b.numero
FROM dservicios a
INNER JOIN eservicios b ON a.id_eservicios = b.id_eservicios
INNER JOIN proyectos c ON b.id_proyecto = c.id_proyecto
where c.id_proyecto = $P{id}]]>
	</queryString>
