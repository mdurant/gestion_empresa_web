	<parameter name="idBoleta" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
EB.id_boleta,
EB.contador,
EB.fecha_creacion,
empresa.RazonSocial,
EB.id_empresa,
EB.Estado,
EB.Total AS TotalG,
EB.Iva AS IvaG,
EB.Neto AS NetoG,
db.Idboleta,
db.IdEboleta,
db.fecha_boleta,
db.Posicion,
db.Codigo,
db.Descripcion,
db.Cantidad,
db.Descuento,
db.Almacen,
db.Neto as NetoP,
db.Iva as IvaP,
db.Total as TotalP
FROM
eboleta AS EB
LEFT OUTER JOIN dboleta as db ON EB.contador = db.IdEboleta
LEFT OUTER JOIN empresa ON EB.id_empresa = empresa.IDEmpresa
WHERE
EB.contador=$P{idBoleta}]]>
	</queryString>
